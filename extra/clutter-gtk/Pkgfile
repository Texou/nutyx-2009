name=clutter-gtk
version=0.10.4
release=1

source=(http://www.clutter-project.org/sources/clutter-gtk/0.10/$name-$version.tar.bz2)


build() {
	cd $name-$version
	./configure --prefix=/usr 
	make
	make DESTDIR=$PKG install
}

